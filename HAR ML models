   "cell_type": "code",
   "execution_count": 455,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Best K: 10\n"
     ]
    }
   ],
   "source": [
    "# K value at which cross-validation performance is maximum\n",
    "max_k_index = np.argmax(knn_cv_scores)\n",
    "best_k = neighbors[max_k_index]\n",
    "print(\"Best K: {}\".format(best_k))"
   ]
